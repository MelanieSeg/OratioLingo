<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90"
    android:toDegrees="90"
    android:pivotX="50%"
    android:pivotY="50%">
    <shape android:shape="line">
        <stroke
            android:width="2dp"
            android:color="#6a4c93"
            android:dashWidth="4dp"
            android:dashGap="2dp" />
    </shape>
</rotate>